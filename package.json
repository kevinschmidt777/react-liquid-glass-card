{
  "name": "react-liquid-glass-card",
  "version": "1.1.1",
  "description": "a tiny react component, giving you a liquid glass card (CSS only!) to use in your projects.",
  "license": "MIT",
  "author": {
    "name": "Kevin Schmidt",
    "email": "kevin@kswebentwicklug.de"
  },
  "type": "module",
